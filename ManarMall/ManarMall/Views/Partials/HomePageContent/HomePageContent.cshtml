<script>
    $(document).ready(function () {
        $('#search').keyup(function () {

            // Search text
            var text = $(this).val();

            // Hide all content class element
            $('.content').hide();

            // Search and show
            $('.content:contains("' + text + '")').show();

        });
    });
</script>

@{
