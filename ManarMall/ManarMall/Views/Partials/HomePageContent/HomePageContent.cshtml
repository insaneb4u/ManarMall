        <div class="movie_index_slider eqheight1" id="slick-images">
            @if (data != null)
            {
                string img1 = "";
                string img2 = "";
                foreach (var item in data.Children())
                {

                    var d1 = item.Children<JProperty>();
                    //you could do a foreach or a linq here depending on what you need to do exactly with the value
                    var d2 = d1.FirstOrDefault(x => x.Name == "addimage1");
                    var d3 = d2.Value; ////This is a JValue type

                    var v1 = d3.Children<JProperty>();
                    //you could do a foreach or a linq here depending on what you need to do exactly with the value
                    var v2 = v1.FirstOrDefault(x => x.Name == "items");
                    var v3 = v2.Value.ToString(); ////This is a JValue type


                    var d4 = item.Children<JProperty>();
                    //you could do a foreach or a linq here depending on what you need to do exactly with the value
                    var d5 = d4.FirstOrDefault(x => x.Name == "addimage2");
                    var d6 = d5.Value; ////This is a JValue type

                    var v4 = d6.Children<JProperty>();
                    //you could do a foreach or a linq here depending on what you need to do exactly with the value
                    var v5 = v4.FirstOrDefault(x => x.Name == "items");
                    var v6 = v5.Value.ToString(); ////This is a JValue type

                    JArray jsonArray = JArray.Parse(v3);
                    dynamic dataq = JObject.Parse(jsonArray[0].ToString());
                    img1 = dataq.imageUrl;

                    var substring1 = img1.Replace("?width=250&height=156.25&mode=crop", "");

                    JArray jsonArray2 = JArray.Parse(v6);
                    dynamic data2 = JObject.Parse(jsonArray2[0].ToString());
                    img2 = data2.imageUrl;
                    var substring2 = img2.Replace("?width=250&height=156.25&mode=crop", "");


